<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- Set Alias -->
    <typeAliases>
        <typeAlias type="com.springboard.user.UserVO" alias="UserVO"/>
        <typeAlias type="com.springboard.blog.PostVO" alias="PostVO"/>
        <typeAlias type="com.springboard.blog.ReplyVO" alias="ReplyVO"/>
